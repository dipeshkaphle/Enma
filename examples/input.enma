/* THis is sum function
It sums from 0 to n inclusive
*/
fn sum(n: int ): int  {
	let i : int = 0;
	let sm : int = 0;
	while( i <= n){
		sm = sm + i;
		i =  i + 1;
	}
	sm;
}

